<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="BrandColor" Color="#409EFF"/>
    <SolidColorBrush x:Key="SecondaryColor" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="SuccessColor" Color="#67C23A"/>
    <SolidColorBrush x:Key="WarningColor" Color="#E6A23C"/>
    <SolidColorBrush x:Key="ErrorColor" Color="#F56C6C"/>
    <SolidColorBrush x:Key="InfoColor" Color="#909399"/>

    <SolidColorBrush x:Key="PrimaryTextColor" Color="#303133"/>
    <SolidColorBrush x:Key="RegularTextColor" Color="#606266"/>
    <SolidColorBrush x:Key="SecondaryTextColor" Color="#909399"/>
    <SolidColorBrush x:Key="PlaceholderTextColor" Color="#C0C4CC"/>

    <SolidColorBrush x:Key="BorderBaseColor" Color="#DCDFE6"/>
    <SolidColorBrush x:Key="BorderLightColor" Color="#E4E7ED"/>
    <SolidColorBrush x:Key="BorderLighterColor" Color="#EBEEF5"/>
